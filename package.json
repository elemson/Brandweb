    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.1.3",
    "gravatar": "^1.8.0",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
